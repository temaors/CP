<Window x:Class="CP.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CP"
        mc:Ignorable="d"
        Title="AdminWindow" Height="333" Width="300.5" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10">

        <Button Content="Управление тренерами" HorizontalAlignment="Center" Margin="0,10" VerticalAlignment="Top" Width="230" Click="Button_Click" Height="29"/>
        <Button Content="Управление абонементами" HorizontalAlignment="Center" Margin="0,10" VerticalAlignment="Top" Width="230" Height="30" Click="Button_Click_1"/>
        <Button Content="Управление пользователями" HorizontalAlignment="Center" Margin="0,10" VerticalAlignment="Top" Width="230" Height="30" Click="Button_Click_2"/>
        <Button Content="Составление отчета" HorizontalAlignment="Center" Margin="0,10" VerticalAlignment="Top" Width="230" Height="30" Click="Button_Click_3"/>
    </StackPanel>
</Window>